<br>
<%= form_with url: "/done", method: :patch do |form| %>
  <%= form.label :date_of_birth, "Date of Birth" %>
  <%= form.date_select :date_of_birth, start_year: 1870 %><br>

  <%= form.label :gender, "Gender" %>
  <%= form.select(:gender, {'male' =>  :male, 'female' => :female, 'non-binary' => :non_binary} )%><br>


  <%= form.label :university, "University" %>
  <%= form.text_field :university %><br>

  <%= form.submit "sign up!" %>
<% end %>
